page {
    background: #f1f1f1;
    padding-bottom: 30rpx;
}

/* 轮播图开始 */
.j-swiper {
    position: relative;
    margin-bottom: 70rpx;
}
.j-swiper::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 42rpx;
    background: linear-gradient(hsla(0, 0%, 100%, 0), #f1f1f1);
    z-index: 1;
}
.j-swiper swiper {
    height: 450rpx;
}
.j-swiper swiper swiper-item image {
    width: 100%;
    height: 100%;
    background: #ebebeb;
}
.j-swiper .search {
    position: absolute;
    left: 30rpx;
    right: 30rpx;
    bottom: -40rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
    color: #c0c4cc;
    text-align: center;
    border-radius: 40rpx;
    z-index: 2;
}
.j-swiper .search .icon {
    position: absolute;
    top: 50%;
    left: 30rpx;
    transform: translateY(-50%);
    width: 40rpx;
    height: 40rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
.j-swiper .loading {
    height: 450rpx;
    position: relative;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-image: linear-gradient(45deg, rgb(28, 187, 180), rgb(141, 198, 63));
}
@keyframes swiper-loading-animation {
    0% {
        transform: rotate(0) scale(0.5);
    }
    100% {
        transform: rotate(360deg) scale(1.35);
    }
}
.j-swiper .loading::after {
    content: '';
    display: block;
    width: 120rpx;
    height: 120rpx;
    background-image: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    animation: swiper-loading-animation 2s infinite alternate;
}

/* 热门文章开始 */
.j-hot {
    margin: 0 30rpx 30rpx;
}
.j-hot .title {
    display: flex;
    align-items: center;
    margin-bottom: 24rpx;
}
.j-hot .title .text {
    font-size: 34rpx;
}
.j-hot .title .icon {
    width: 60rpx;
    height: 60rpx;
    min-width: 60rpx;
    min-height: 60rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin-right: 10rpx;
    background-image: url(data:image/svg+xml;base64,);
}
.j-hot .scroll {
    white-space: nowrap;
}
.j-hot .scroll .item {
    width: 340rpx;
    position: relative;
    display: inline-block;
    margin-right: 30rpx;
}
.j-hot .scroll .item:last-child {
    margin-right: 0;
}
.j-hot .scroll .item image {
    width: 100%;
    height: 180rpx;
    border-radius: 10rpx 10rpx 0 0;
}
.j-hot .scroll .item .text {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: center;
    padding: 14rpx 20rpx;
    background: #fff;
    border-radius: 0 0 10rpx 10rpx;
}
.j-hot .scroll .item .num::after {
    content: '';
    border-style: solid;
    border-width: 8px;
    position: absolute;
    bottom: -16px;
    right: 0;
    z-index: -1;
    border-color: #fa3534 transparent transparent;
}
.j-hot .scroll .item .num {
    position: absolute;
    top: 8px;
    right: -8px;
    padding: 0 8px;
    font-size: 24rpx;
    background: #fa3534;
    font-weight: 500;
    color: #fff;
    border-radius: 2px 2px 0 2px;
}
@keyframes hot-loading-animation {
    0% {
        transform: scale(0.85);
    }
    100% {
        transform: scale(1);
    }
}
.j-hot .scroll.loading .image {
    width: 100%;
    height: 180rpx;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
    background: #ebebeb;
    animation: hot-loading-animation 0.5s infinite alternate;
}
.j-hot .scroll.loading .text {
    width: 100%;
    height: 20px;
    border-radius: 4rpx;
    background: #ebebeb;
    animation: hot-loading-animation 0.5s infinite alternate;
}

/* 通知 */
.j-notice {
    display: flex;
    align-items: center;
    margin: 0 30rpx 30rpx;
    background: #fff;
    height: 76rpx;
    line-height: 76rpx;
    border-radius: 38rpx;
    padding: 0 30rpx;
}

.j-notice .icon {
    width: 34rpx;
    height: 34rpx;
    min-width: 34rpx;
    min-height: 34rpx;
    background-image: url(data:image/svg+xml;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin-right: 10rpx;
}

.j-notice .text {
    flex: 1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #909399;
}

/* 列表 */
.j-list {
    margin: 0 30rpx 30rpx;
}
.j-list .item {
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    margin-bottom: 30rpx;
}
.j-list .item:last-child {
    margin-bottom: 0;
}
.j-list .item .imager image {
    width: 100%;
    height: 290rpx;
}
.j-list .item .describe {
    padding: 24rpx;
}
.j-list .item .describe .title {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 32rpx;
    margin-bottom: 8rpx;
}
.j-list .item .describe .title.empty-desc {
    margin-bottom: 24rpx;
}
.j-list .item .describe .desc {
    font-size: 24rpx;
    color: #b3b3b3;
    line-height: 2;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 24rpx;
}
.j-list .item .describe .meta {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 2rpx solid #f2f6fc;
    padding-top: 24rpx;
}
.j-list .item .describe .meta .term {
    display: flex;
    align-items: center;
}
.j-list .item .describe .meta .term .icon {
    width: 28rpx;
    height: 28rpx;
    min-width: 28prx;
    min-height: 28rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-right: 10rpx;
    opacity: 0.5;
}
.j-list .item .describe .meta .term .icon-1 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .icon-2 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .icon-3 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .text {
    color: #b3b3b3;
    font-size: 24rpx;
}
.j-list .item.loading {
    background: none;
}
@keyframes list-imager-loading-animation {
    0% {
        transform: scale(0.75);
    }
    100% {
        transform: scale(1);
    }
}
.j-list .item.loading .imager {
    width: 100%;
    height: 290rpx;
    background: #ebebeb;
    animation: list-imager-loading-animation 0.5s infinite alternate;
}
@keyframes list-title-loading-animation {
    0% {
        width: 25%;
    }
    100% {
        width: 80%;
    }
}
.j-list .item.loading .describe .title {
    width: 25%;
    height: 32rpx;
    background: #ebebeb;
    animation: list-title-loading-animation 0.5s infinite alternate;
    margin-bottom: 24rpx;
}
.j-list .item.loading .describe .meta {
    border-top: none;
    padding-top: 0;
}
.j-list .item.loading .describe .meta .term {
    flex: 1;
}
@keyframes list-meta-loading-animation {
    0% {
        width: 20%;
    }
    100% {
        width: 80%;
    }
}
.j-list .item.loading .describe .meta .term .text {
    width: 50%;
    height: 24rpx;
    background: #ebebeb;
    animation: list-meta-loading-animation 0.5s infinite alternate;
}

/* 全部加载完 */
.j-end {
    text-align: center;
    margin: 0 30rpx;
    color: #c0c4cc;
}
