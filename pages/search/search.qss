page {
    padding-top: 140rpx;
}

.j-search {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    background: #409eff;
    padding: 30rpx;
}

.j-search .icon {
    position: absolute;
    top: 50%;
    left: 60rpx;
    transform: translateY(-50%);
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
}

.j-search input {
    width: 100%;
    height: 80rpx;
    background: #fff;
    border-radius: 40rpx;
    padding: 0 90rpx;
    color: #606266;
}

.j-group {
    padding: 0 30rpx;
    margin-bottom: 60rpx;
}

.j-group .title {
    position: relative;
    border-bottom: 2rpx solid#ebeef5;
    padding: 0 30rpx 20rpx;
    height: 90rpx;
    line-height: 90rpx;
    margin-bottom: 30rpx;
    font-size: 32rpx;
}

.j-group .title .clear {
    position: absolute;
    top: 50%;
    right: 30rpx;
    transform: translateY(-50%);
    width: 30rpx;
    height: 30rpx;
}

.j-group .title::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    width: 10rpx;
    height: 10rpx;
    border-radius: 50%;
}

.j-group .title.success::before {
    background: #67c23a;
}

.j-group .title.danger::before {
    background: #f56c6c;
}

.j-group .list {
    display: flex;
    flex-wrap: wrap;
}

.j-group .list .item {
    background: #f2f6fc;
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 8rpx;
    padding: 0 20rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    color: #909399;
    font-size: 24rpx;
}

.j-group .empty {
    padding-top: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #909399;
    font-size: 24rpx;
}

.j-group .empty image {
    width: 128rpx;
    height: 128rpx;
    margin-bottom: 30rpx;
}

@keyframes tags-loading-animation {
    0% {
        transform: scale(0.85);
    }
    100% {
        transform: scale(1);
    }
}

.j-group .list.loading .item {
    background: #ebebeb;
    width: 25%;
    animation: tags-loading-animation 0.5s infinite alternate;
}
