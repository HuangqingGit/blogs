page {
    padding-top: 118rpx;
    background: #f1f1f1;
}

/* Tab栏 */
.j-tabbar {
    position: relative;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    display: flex;
    align-items: center;
    background: #fff;
    z-index: 99;
    border-bottom: 2rpx solid #ebeef5;
}
.j-tabbar .item {
    text-align: center;
    flex: 1;
    height: 88rpx;
    line-height: 88rpx;
    padding: 0 10rpx;
}
.j-tabbar .item .text {
    color: #606266;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.j-tabbar .item.active .text {
    color: #303133;
    font-weight: 500;
}
.j-tabbar .line {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 6rpx;
    border-radius: 3rpx;
    background: #409eff;
    transition: transform 0.35s;
}

/* 列表 */
.j-list {
    margin: 0 30rpx 30rpx;
}
.j-list .empty {
    padding-top: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #c0c4cc;
}
.j-list .empty image {
    width: 128rpx;
    height: 128rpx;
    margin-bottom: 20rpx;
}
.j-list .item {
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    margin-bottom: 30rpx;
}
.j-list .item:last-child {
    margin-bottom: 0;
}
.j-list .item .imager image {
    width: 100%;
    height: 290rpx;
}
.j-list .item .describe {
    padding: 24rpx;
}
.j-list .item .describe .title {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 32rpx;
    margin-bottom: 8rpx;
}
.j-list .item .describe .title.empty-desc {
    margin-bottom: 24rpx;
}
.j-list .item .describe .desc {
    font-size: 24rpx;
    color: #b3b3b3;
    line-height: 2;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 24rpx;
}
.j-list .item .describe .meta {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 2rpx solid #f2f6fc;
    padding-top: 24rpx;
}
.j-list .item .describe .meta .term {
    display: flex;
    align-items: center;
}
.j-list .item .describe .meta .term .icon {
    width: 28rpx;
    height: 28rpx;
    min-width: 28prx;
    min-height: 28rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-right: 10rpx;
    opacity: 0.5;
}
.j-list .item .describe .meta .term .icon-1 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .icon-2 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .icon-3 {
    background-image: url(data:image/svg+xml;base64,);
}
.j-list .item .describe .meta .term .text {
    color: #b3b3b3;
    font-size: 24rpx;
}
.j-list .item.loading {
    background: none;
}
@keyframes list-imager-loading-animation {
    0% {
        transform: scale(0.75);
    }
    100% {
        transform: scale(1);
    }
}
.j-list .item.loading .imager {
    width: 100%;
    height: 290rpx;
    background: #ebebeb;
    animation: list-imager-loading-animation 0.5s infinite alternate;
}
@keyframes list-title-loading-animation {
    0% {
        width: 25%;
    }
    100% {
        width: 80%;
    }
}
.j-list .item.loading .describe .title {
    width: 25%;
    height: 32rpx;
    background: #ebebeb;
    animation: list-title-loading-animation 0.5s infinite alternate;
    margin-bottom: 24rpx;
}
.j-list .item.loading .describe .meta {
    border-top: none;
    padding-top: 0;
}
.j-list .item.loading .describe .meta .term {
    flex: 1;
}
@keyframes list-meta-loading-animation {
    0% {
        width: 20%;
    }
    100% {
        width: 80%;
    }
}
.j-list .item.loading .describe .meta .term .text {
    width: 50%;
    height: 24rpx;
    background: #ebebeb;
    animation: list-meta-loading-animation 0.5s infinite alternate;
}

/* 全部加载完 */
.j-end {
    text-align: center;
    margin: 0 30rpx;
    color: #c0c4cc;
}
